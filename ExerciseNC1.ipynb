   "source": [
    "## ## NC1.4. Mô phỏng, trực quan hóa cây quyết định"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "f233b76b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
